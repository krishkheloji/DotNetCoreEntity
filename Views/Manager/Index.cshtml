@{
}

@model List<Manager>

<a class="btn btn-info" asp-action="AddManager">Add Manager</a><br /><br />

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Action</th>

        </tr>
    </thead>
    <tbody>
        @foreach(var d in Model)
        {
            <tr>
                <th scope="row">@d.mid</th>
                <td>@d.mname</td>
               
            </tr>
        }
        
        
    </tbody>
</table>